<div class="container">
<div class="row">
<div class="panel panel-default">
  <div class="panel-heading">
    <h3>Free Code Camp Fargo, ND Leaderboard</h3>
  </div>
  <table class="table table-striped table-responsive">
    <thead>
      <tr>
        <th>#</th>
        <th></th>
        <th>Username</th>
        <th>Points</th>
        <%if current_user.admin?%>
        <th></th>
        <th></th>
        <%end%>
      </tr>
    </thead>
    <tbody>
      <% @campers.each do |camper| %>
        <tr>
          <%= render camper %>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
</div>
</div>
