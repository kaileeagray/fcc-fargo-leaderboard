<h1>New Free Code Camp Camper Profile</h1>

  <%= form_for @camper_profile, controller: 'attractions', action: 'create' do |f| %>

  <div class="field">
    <%= f.label :username %>
    <%= f.text_field :username %>
  </div>

  <div class="field">
    <%= f.label :points, "Total Points" %>
    <%= f.number_field :points %>
  </div>

  <%= f.submit "Create Camper Profile" %>

<% end %>
